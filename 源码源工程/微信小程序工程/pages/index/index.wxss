page {
    align-items: center;
}

.container {
    margin-top: 30px;
    width: 450rpx;
    height: 450rpx;
    border: 20rpx solid rgba(122, 66, 20, 0.767);
    overflow: hidden;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    align-content: space-between;
    border-radius: 20rpx;
}

.v1 {
    width: 150rpx;
    height: 150rpx;
    color:rgba(122, 66, 20, 0.973);
    font-size:70rpx;
    font-weight: 600;
    position: relative;
    border-radius: 10rpx;
    border-color: rgba(80, 44, 14, 0.973);
}

.v1 view {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 10;
}
.v1 text {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 10;
}

.v2 {
    width: 150rpx;
    height: 150rpx;
    background: #fff;
}

.v3 {
    color: #884f26;
    font-weight: 550;
    font-size: 32rpx;
    margin: 20rpx 0;
}

.v4 {
    color: rgb(134, 77, 35);
    font-weight: 550;
    font-size: 32rpx;
    margin-bottom:15rpx;
}

.v5 {
    color: rgb(134, 77, 35);
    font-weight: 550;
    font-size: 32rpx;
    margin-bottom: 15rpx;
    margin-top: 2px;
}
.game {
    width: 300rpx;
    height: 90rpx;
    background: rgba(122, 66, 20, 0.767);
    color: #fff;
    border-radius: 20rpx;
    font-size: 40rpx;
    font-weight: 550;
    margin-top: 3px;
}

.reset {
    width: 300rpx;
    height: 90rpx;
    background: rgba(122, 66, 20, 0.767);
    color: #fff;
    border-radius: 20rpx;
    font-size: 40rpx;
    font-weight: 550;
    margin: 20rpx 0;
    margin-bottom: 5px;
}